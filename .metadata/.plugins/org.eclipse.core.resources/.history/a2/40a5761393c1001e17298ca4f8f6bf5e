package com.cts.banking.utility;

import java.util.Hashtable;
import java.util.Random;

import com.cts.banking.models.Customer;
import com.cts.banking.models.Transaction;

public class HashtableDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Hashtable<Customer,Transaction> transactions=
				new Hashtable<Customer,Transaction>();
		
		Customer customer=null;
		for(int i=0;i<100;i++) {
			
			for(int j=0;j<(1+new Random().nextInt(5));i++) {
				transactions.put(customer, null)
				
			}
			
			
		}
		
		

	}

}
